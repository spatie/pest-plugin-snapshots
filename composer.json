{
    "name": "spatie/pest-plugin-snapshots",
    "description": "A package for snapshot testing in Pest",
    "keywords": [
        "spatie",
        "pest",
        "plugin",
        "snapshots"
    ],
    "homepage": "https://github.com/spatie/pest-plugin-snapshots",
    "license": "MIT",
    "require": {
        "php": "^7.4",
        "pestphp/pest": "dev-master",
        "spatie/phpunit-snapshot-assertions": "^4.2.2"
    },
    "autoload": {
        "psr-4": {
            "Spatie\\Snapshots\\Concerns\\": "./src/Concerns/",
            "Spatie\\Snapshots\\Laravel\\": "./src/Laravel/"
        },
        "files": [
            "src/Functions.php"
        ]
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16.3",
        "illuminate/support": "^7.0",
        "illuminate/testing": "^7.0",
        "orchestra/testbench": "^5.0",
        "pestphp/pest-plugin-laravel": "^0.2",
        "fzaninotto/faker": "^1.9"
    },
    "scripts": {
        "test": "pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "Spatie\\Snapshots\\Laravel\\SnapshotsServiceProvider"
            ]
        }
    }
}
